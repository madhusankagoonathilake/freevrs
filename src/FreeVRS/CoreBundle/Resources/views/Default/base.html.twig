<!DOCTYPE html>
<html>
    <head>
        <!--Import materialize.css-->
        <link type="text/css" rel="stylesheet" href="{{ asset('bundles/freevrscore/materialize/css/materialize.min.css') }}"  media="screen,projection"/>
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" />

        {% block stylesheets %}
        {% endblock %}

        <!--Let browser know website is optimized for mobile-->
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    </head>

    <body>

        <nav>
            <div class="row">

                <div class="nav-wrapper">
                    <div class="container">
                        <a href="#!" class="brand-logo">FreeVRS</a>
                       {% block nav_menu %}{% endblock %}
                    </div>
                </div>

            </div>
        </nav>

        {% block content %} {% endblock %}
        <!--Import jQuery before materialize.js-->
        <script type="text/javascript" src="{{ asset('bundles/freevrscore/jquery/jquery-2.1.1.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/freevrscore/materialize/js/materialize.min.js') }}"></script>
        
        {% block javascripts %}
        {% endblock %}
    </body>
</html>
